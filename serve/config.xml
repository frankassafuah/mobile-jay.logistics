<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jaylogistic.mobile" version="4.0.2" versionCode="402" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Jay Logistic International</name>
    <description>
        The Best in Man and Van Services
    </description>
    <author email="info@easycreativesolutions.com" href="http://cordova.io">
        Easy Creative Solutions Ltd
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="maps:*" />
    <allow-navigation href="https://www.jaylogistic.com/*" />
    <allow-navigation href="https://www.jaylogistic.com/mobile/*" />
    <allow-navigation href="https://www.jaylogistic.com/mobile/*/*" />
    <allow-navigation href="https://www.jaylogistic.com/mobile/*/*/*" />
    <allow-navigation href="https://www.jaylogistic.com/status/*" />
    <allow-navigation href="https://www.jaylogistic.com/app/*" />
    <allow-navigation href="https://www.jaylogistic.com/app/*/*" />
    <allow-navigation href="https://www.jaylogistic.com/app/*/*/*" />
    <access launch-external="yes" origin="tel:*" />
    <icon height="1024" src="icon.png" width="1024" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <gap:plugin name="cordova-plugin-device" source="npm" />
    <gap:plugin name="cordova-plugin-inappbrowser" source="npm" />
    <gap:plugin name="cordova-plugin-network-information" source="npm" />
    <gap:plugin name="cordova-plugin-media" source="npm" spec="5.0.2" />
    <gap:plugin name="cordova.plugins.diagnostic" source="npm" spec="4.0.0" />
    <gap:plugin name="cordova-plugin-buildinfo" source="npm" spec="2.0.1" />
    <gap:config-file parent="/manifest" platform="android">
        <supports-screens android:largeScreens="true" android:smallScreens="true" android:xlargeScreens="true" />
        <application android:theme="@android:style/Theme.NoTitleBar">
        </application>
    </gap:config-file>
    <config-file parent="NSCameraUsageDescription" platform="ios" target="*-Info.plist">
        <string>Enables us to access your camera to take photos for your profile or of your van.</string>
    </config-file>
    <config-file parent="NSPhotoLibraryUsageDescription" platform="ios" target="*-Info.plist">
        <string>Enables us to access your photo library to save/use images for your profile, van or company image.</string>
    </config-file>
    <config-file parent="NSPhotoLibraryAddUsageDescription" platform="ios" target="*-Info.plist">
        <string>Enables us to access your photo library to save images to your phone.</string>
    </config-file>
    <config-file parent="NSBluetoothAlwaysUsageDescription" platform="ios" target="*-Info.plist">
        <string>Enables us to access bluetooth for support calls and sharing of files </string>
    </config-file>
    <preference name="prerendered-icon" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="1000" />
    <preference name="fullscreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="android-minSdkVersion" value="22" />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="phonegap-version" value="cli-9.0.0" />
</widget>
